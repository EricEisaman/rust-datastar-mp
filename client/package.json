{
  "name": "rust-datastar-mp-client",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd ../server/api && cargo run",
    "dev:client": "vite",
    "build": "vite build",
    "lint:only": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"src/**/*.{js,ts,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,json,css,scss,md}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babylonjs/core": "^8.40.1",
    "@babylonjs/gui": "^8.40.1",
    "@babylonjs/loaders": "^8.40.1",
    "@babylonjs/materials": "^8.40.1",
    "@starfederation/datastar": "^1.0.0-beta.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  }
}
